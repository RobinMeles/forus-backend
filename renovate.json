{
  "extends": [
    "config:base"
  ],
  "rangeStrategy": "pin",
  "baseBranches": [
    "develop"
  ],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "schedule": [
    "after 10pm and before 6am on every weekday"
  ],
  "packageRules": [
    {
      "packagePatterns": ["^laravel/"],
      "enabled": false
    }
  ],
  "packageRules": [
    {
      "packageNames": ["aws-sdk"],
      "schedule": ["after 9pm on sunday"]
    }
  ],
  "composer": {
    "enabled": true
  },
  "lockFileMaintenance": {
    "enabled": true
  }
}
